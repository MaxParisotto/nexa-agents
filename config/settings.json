{
  "lmStudio": {
    "apiUrl": "http://localhost:1234",
    "defaultModel": "qwen2.5-7b-instruct-1m"
  },
  "ollama": {
    "apiUrl": "http://localhost:11434",
    "defaultModel": "deepscaler:latest"
  },
  "projectManager": {
    "apiUrl": "http://localhost:1234",
    "model": "qwen2.5-7b-instruct-1m",
    "serverType": "lmStudio",
    "parameters": {
      "temperature": 0.7,
      "topP": 0.9,
      "topK": 40,
      "repeatPenalty": 1.1,
      "maxTokens": 1024,
      "contextLength": 4096
    }
  },
  "features": {
    "enableFileUploads": false,
    "enableVoiceInput": false,
    "chatWidget": true,
    "projectManagerAgent": true,
    "taskManagement": true,
    "loggingSystem": true,
    "notifications": true,
    "metrics": true,
    "autoSave": true,
    "debugMode": true,
    "experimentalFeatures": true
  },
  "uplink": {
    "websocket": {
      "enabled": true,
      "host": "localhost",
      "port": 8080
    },
    "restApi": {
      "enabled": true,
      "host": "localhost",
      "port": 8081
    }
  },
  "nodeEnv": "development",
  "port": "3001"
}
