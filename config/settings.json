{
  "lmStudio": {
    "apiUrl": "http://localhost:1234",
    "defaultModel": "qwen2.5-0.5b-instruct-mlx"
  },
  "ollama": {
    "apiUrl": "http://localhost:11434",
    "defaultModel": "deepscaler:latest"
  },
  "projectManager": {
    "apiUrl": "http://localhost:1234",
    "model": "qwen2.5-0.5b-instruct-mlx",
    "serverType": "lmStudio",
    "parameters": {
      "temperature": 0.7,
      "topP": 0.9,
      "topK": 40,
      "repeatPenalty": 1.1,
      "maxTokens": 1024,
      "contextLength": 4096
    }
  },
  "features": {
    "enableFileUploads": false,
    "enableVoiceInput": false,
    "chatWidget": true,
    "projectManagerAgent": true,
    "taskManagement": true,
    "loggingSystem": true,
    "notifications": true,
    "metrics": true,
    "autoSave": true,
    "debugMode": true,
    "experimentalFeatures": true
  },
  "nodeEnv": "development",
  "port": "3001",
  "uplink": {
    "websocket": {
      "enabled": true,
      "port": 3002,
      "apiPath": "/api/v1/ws",
      "restApi": {
        "enabled": true,
        "path": "/api/v1/rest",
        "methods": ["GET", "POST", "PUT", "DELETE"]
      },
      "authentication": {
        "type": "apiKey",
        "header": "X-API-KEY"
      },
      "rateLimiting": {
        "enabled": true,
        "maxRequests": 100,
        "windowMs": 60000
      }
    },
    "openai": {
      "enabled": true,
      "apiKey": "",
      "endpoint": "https://api.openai.com/v1",
      "models": {
        "gpt-4": {
          "maxTokens": 8192,
          "temperature": 0.7,
          "topP": 1.0
        },
        "gpt-3.5-turbo": {
          "maxTokens": 4096,
          "temperature": 0.7,
          "topP": 1.0
        }
      },
      "actions": {
        "enabled": true,
        "auth": {
          "type": "apiKey",
          "header": "Authorization"
        },
        "rateLimiting": {
          "enabled": true,
          "maxRequests": 60,
          "windowMs": 60000
        }
      }
    }
  }
}
